
/*STAR PATTERN*/
#include <stdio.h>
int main() {
   int i, j, rows;
   scanf("%d", &rows);
   for (i = 1; i <= rows; ++i) {
      for (j = 1; j <= i; ++j) {
         printf("*");
      }
      printf("\n");
   }
   return 0;
}






/*ABSOLUTE VALUE OF NUMBER*/

int main(){
	int num;
	scanf("%d", &num);    
	if(num< 0)
		{
			num=-num;
			printf("%d",num);
		}
	else
		printf("%d",num);
}





/*CELCIUS TO FARENHIET*/


int main(){
	int num,f;
	scanf("%d", &num);
	f = (1.8*num)+32;
	printf("%d",f);
}





/*SOLUTION OF QUDRATIC EQUATION*/


import math

a=int(input())
b=int(input())
c=int(input())

d = (b**2) - (4*a*c)

# find two solutions
sol1 = (-b-math.sqrt(d))/(2*a)
sol2 = (-b+math.sqrt(d))/(2*a)
    
print(int(sol2))
print(int(sol1))





/*FACTORIAL OF NUMBER*/

int main(){
	int num,i,f=1;
	if(num>0)
	{
		scanf("%d", &num);              		
		for(i=1;i<=num;i++)    
		{
			f*=i;
		} 
		printf("%d",f); 
	}
	else
		printf("%d",1);
}






/*  SUM OF DIGIT OF FACTORIAL*/

#include <stdio.h>

int main(){
	

	int num,i,f=1,a,b=0;
	
		scanf("%d", &num);  
	if(num>0)
	{            			// Reading input from STDIN
		for(i=1;i<=num;i++)    
		{
			f*=i;
		} 
        while(f!=0)
        {
            f= f/10;
            b++;
        }
		printf("%d",b); // Writing output to STDOUT
	}
	else
		printf("%d",1);
}






/*GP TERM*/

#include <stdio.h>
#include <math.h>

int main()
{
	int a,r,n,t,p;
	scanf("%d", &a);   
	scanf("%d", &r); 
	scanf("%d", &n); 
	p =pow(r,n-1);
	         
	t= a*p;
	printf("%d",t);
}

















